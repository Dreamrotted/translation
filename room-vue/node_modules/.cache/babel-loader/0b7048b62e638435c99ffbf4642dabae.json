{"ast":null,"code":"export default {\n  data() {\n    // æ¨¡æ‹Ÿè‡ªä¹ å®¤åˆ—è¡¨\n    const mockRoomList = [{\n      id: 1,\n      roomNumber: '101',\n      location: 'Aæ ‹1æ¥¼'\n    }, {\n      id: 2,\n      roomNumber: '102',\n      location: 'Aæ ‹1æ¥¼'\n    }, {\n      id: 3,\n      roomNumber: '201',\n      location: 'Bæ ‹2æ¥¼'\n    }, {\n      id: 4,\n      roomNumber: '202',\n      location: 'Bæ ‹2æ¥¼'\n    }, {\n      id: 5,\n      roomNumber: '301',\n      location: 'Cæ ‹3æ¥¼'\n    }, {\n      id: 6,\n      roomNumber: '302',\n      location: 'Cæ ‹3æ¥¼'\n    }]; // ç”Ÿæˆæ¨¡æ‹Ÿå®æ—¶æ•°æ®\n\n    const generateRealTimeData = () => {\n      return mockRoomList.map(room => {\n        const totalSeats = Math.floor(Math.random() * 80) + 40; // 40-120ä¸ªåº§ä½\n\n        const occupiedSeats = Math.floor(Math.random() * totalSeats * 0.8) + Math.floor(totalSeats * 0.2); // 20%-100%\n\n        const usageRate = (occupiedSeats / totalSeats * 100).toFixed(1);\n        return { ...room,\n          totalSeats,\n          occupiedSeats,\n          availableSeats: totalSeats - occupiedSeats,\n          usageRate\n        };\n      });\n    };\n\n    return {\n      // å½“å‰æ—¶é—´ï¼ˆç”¨äºæ¼”ç¤ºï¼‰\n      currentTime: new Date().toLocaleTimeString(),\n      // æ¨¡æ‹Ÿæ•°æ®\n      roomList: mockRoomList,\n      realTimeData: {\n        rooms: generateRealTimeData(),\n        updateTime: new Date().toLocaleString()\n      },\n      // æ¼”ç¤ºæ§ä»¶\n      comparisonDateRange: null,\n      selectedRoomId: mockRoomList[0]?.id,\n      trendDate: new Date().toISOString().split('T')[0],\n      // è‡ªåŠ¨åˆ·æ–°è®¡æ—¶å™¨\n      autoRefreshTimer: null\n    };\n  },\n\n  methods: {\n    // æ˜¾ç¤ºæ—¥æœŸé€‰æ‹©å™¨æ›´æ”¹æ¼”ç¤º\n    showDateChangeDemo(message) {\n      this.$message({\n        message: `æ¼”ç¤º: ${message}`,\n        type: 'info',\n        duration: 1500\n      });\n    },\n\n    // æ˜¾ç¤ºä¸‹æ‹‰æ¡†é€‰æ‹©æ¼”ç¤º\n    showSelectChangeDemo() {\n      this.$message({\n        message: 'æ¼”ç¤º: è‡ªä¹ å®¤é€‰æ‹©å·²æ›´æ–°',\n        type: 'info',\n        duration: 1500\n      });\n    },\n\n    // åˆ·æ–°æ¼”ç¤ºæ•°æ®\n    refreshDemoData() {\n      // æ›´æ–°å½“å‰æ—¶é—´\n      this.currentTime = new Date().toLocaleTimeString(); // é‡æ–°ç”Ÿæˆæ¨¡æ‹Ÿæ•°æ®\n\n      this.realTimeData = {\n        rooms: this.roomList.map(room => {\n          const totalSeats = Math.floor(Math.random() * 80) + 40;\n          const occupiedSeats = Math.floor(Math.random() * totalSeats * 0.8) + Math.floor(totalSeats * 0.2);\n          const usageRate = (occupiedSeats / totalSeats * 100).toFixed(1);\n          return { ...room,\n            totalSeats,\n            occupiedSeats,\n            availableSeats: totalSeats - occupiedSeats,\n            usageRate\n          };\n        }),\n        updateTime: new Date().toLocaleString()\n      };\n      this.$message({\n        message: 'æ¼”ç¤ºæ•°æ®å·²åˆ·æ–°',\n        type: 'success',\n        duration: 1500\n      });\n    },\n\n    // ç”Ÿæˆæ¼”ç¤ºå›¾è¡¨æ•°æ®\n    generateDemoChartData() {\n      this.$message({\n        message: 'æ­£åœ¨ç”ŸæˆæŸ±çŠ¶å›¾æ¼”ç¤ºæ•°æ®...',\n        type: 'info',\n        duration: 1000\n      }); // æ¨¡æ‹Ÿå»¶è¿Ÿæ•ˆæœ\n\n      setTimeout(() => {\n        this.$message({\n          message: 'æŸ±çŠ¶å›¾æ¼”ç¤ºæ•°æ®å·²ç”Ÿæˆ',\n          type: 'success',\n          duration: 1500\n        });\n      }, 800);\n    },\n\n    // ç”Ÿæˆæ¼”ç¤ºè¶‹åŠ¿æ•°æ®\n    generateDemoTrendData() {\n      this.$message({\n        message: 'æ­£åœ¨ç”ŸæˆæŠ˜çº¿å›¾æ¼”ç¤ºæ•°æ®...',\n        type: 'info',\n        duration: 1000\n      }); // æ¨¡æ‹Ÿå»¶è¿Ÿæ•ˆæœ\n\n      setTimeout(() => {\n        this.$message({\n          message: 'æŠ˜çº¿å›¾æ¼”ç¤ºæ•°æ®å·²ç”Ÿæˆ',\n          type: 'success',\n          duration: 1500\n        });\n      }, 800);\n    },\n\n    // æŸ¥çœ‹æ‰€æœ‰æ•°æ®\n    showAllData() {\n      this.$alert(`\n        <div style=\"max-height: 400px; overflow-y: auto;\">\n          <h4>å½“å‰æ¼”ç¤ºæ•°æ®æ¦‚è§ˆ</h4>\n          <p><